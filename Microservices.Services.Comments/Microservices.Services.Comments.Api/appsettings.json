{
  "application": {
    "name": "Comments Service",
    "service": "comments-service",
    "version": "1.0"
  },
  "logging": {
    "level": "Information",
    "excludePaths": ["/", "/ping", "/metrics"],
    "console": {
      "enabled": false
    },
    "file": {
      "enabled": false
    },
    "seq": {
      "enabled": true,
      "url": "http://localhost:5341",
      "apiKey": "supersecretandsecurekeycomments"
    }
  },
  "mongo": {
    "connectionString": "mongodb://localhost:27017",
    "database": "Comments"
  },
  "prometheus": {
    "enabled": true,
    "endpoint": "/metrics"
  },
  "metrics": {
    "enabled": true,
    "influxEnabled": false,
    "prometheusEnabled": true,
    "prometheusFormatter":"",
    "influxUrl": "http://localhost:8086",
    "database": "microservices",
    "interval": 5,
    "tags": {
      "app": "comments-services",
      "env": "dev",
      "server": "local"
    }
  },
  "rabbitMq": {
    "connectionName": "comments-service",
    "retries": 3,
    "retryInterval": 2,
    "logger": {
      "enabled": true,
      "logConnectionStatus": true
    },
    "username": "guest",
    "password": "guest",
    "virtualHost": "/",
    "port": 5672,
    "hostnames": [
      "localhost"
    ],
    "requestedConnectionTimeout": "00:00:30",
    "requestedHeartbeat": "00:01:00",
    "socketReadTimeout": "00:00:30",
    "socketWriteTimeout": "00:00:30",
    "continuationTimeout": "00:00:20",
    "handshakeContinuationTimeout": "00:00:10",
    "networkRecoveryInterval": "00:00:05",
    "exchange": {
      "declare": true,
      "durable": true,
      "autoDelete": false,
      "type": "topic",
      "name": "comments"
    },
    "queue": {
      "declare": true,
      "durable": true,
      "exclusive": false,
      "autoDelete": false,
      "template": "comments-service/{{exchange}}.{{message}}"
    }
  }
}
