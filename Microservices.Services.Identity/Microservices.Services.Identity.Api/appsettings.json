{
  "application": {
    "name": "Identity Service",
    "service": "identity-service",
    "version": "1.0"
  },
  "logging": {
    "level": "Information",
    "excludePaths": ["/", "/ping", "/metrics"],
    "console": {
      "enabled": false
    },
    "file": {
      "enabled": false
    },
    "seq": {
      "enabled": true,
      "url": "http://localhost:5341",
      "apiKey": "supersecretandsecurekeyposts"
    }
  },
  "prometheus": {
    "enabled": true,
    "endpoint": "/metrics"
  },
  "metrics": {
    "enabled": true,
    "influxEnabled": false,
    "prometheusEnabled": true,
    "prometheusFormatter":"",
    "influxUrl": "http://localhost:8086",
    "database": "microservices",
    "interval": 5,
    "tags": {
      "app": "identity-services",
      "env": "dev",
      "server": "local"
    }
  }
}
